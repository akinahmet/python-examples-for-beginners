    "def count_letter(sentence):\n",
    "    dictionary = {}\n",
    "    for i in sentence:\n",
    "        if i not in dictionary:\n",
    "            dictionary[i]=1\n",
    "        else:\n",
    "            dictionary[i]+=1\n",
    "    return (dictionary)\n",
